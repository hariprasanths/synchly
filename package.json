{
    "name": "synchly",
    "version": "1.1.0",
    "description": "A CLI to automate database backups",
    "homepage": "https://github.com/hariprasanths/synchly",
    "repository": {
        "type": "git",
        "url": "https://github.com/hariprasanths/synchly.git"
    },
    "author": {
        "name": "Hariprasanth S",
        "email": "shhariprasanth@gmail.com",
        "url": "https://hariprasanths.github.io"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/hariprasanths/synchly/issues"
    },
    "main": "src/backup.js",
    "scripts": {
        "postinstall": "node scripts/postinstall.js",
        "postuninstall": "node scripts/postuninstall.js",
        "fmt": "prettier --print-width 120 --no-bracket-spacing --single-quote --tab-width 4 --write scripts src",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "pre-commit": [
        "fmt"
    ],
    "bin": {
        "synchly": "bin/synchly"
    },
    "engines": {
        "node": ">=8"
    },
    "files": [
        "/bin",
        "/scripts",
        "/src"
    ],
    "preferGlobal": true,
    "keywords": [
        "cli",
        "automatic",
        "backup",
        "database",
        "sync",
        "synchronise",
        "recurring",
        "scheduler",
        "remote-sync",
        "notifications",
        "mongodb",
        "mysql",
        "postgresql",
        "google-drive",
        "sftp"
    ],
    "dependencies": {
        "arg": "^4.1.3",
        "cli-table3": "^0.6.0",
        "conf": "^6.2.4",
        "googleapis": "^49.0.0",
        "inquirer": "^7.1.0",
        "inquirer-datepicker-prompt": "^0.4.2",
        "keytar": "^6.0.1",
        "mongoose": "^5.9.11",
        "mysql": "^2.18.1",
        "node-cron": "^2.0.3",
        "nodemailer": "^6.4.6",
        "omelette": "^0.4.12",
        "ora": "^4.0.4",
        "pg": "^8.5.1",
        "redis": "^3.0.2",
        "ssh2-sftp-client": "^5.1.2"
    },
    "devDependencies": {
        "pre-commit": "^1.2.2",
        "prettier": "^2.0.5"
    }
}
