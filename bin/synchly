#!/usr/bin/env node

const omelette = require('omelette');
const completion = omelette('synchly').tree({
    '--config': ['db', 'remote-sync', 'smtp'],
    '--enable': ['remote-sync', 'smtp'],
    '--disable': ['remote-sync', 'smtp'],
    '--version':[],
    '--help':[],
});
completion.init();

require('../src/cli').cli(process.argv);