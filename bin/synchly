#!/usr/bin/env node

const omelette = require('omelette');
const completion = omelette('synchly').tree({
    '--config': {
        'db': ['--debug', '--stacktrace'],
        'remote-sync': ['--debug', '--stacktrace'],
        'smtp': ['--debug', '--stacktrace']
    },
    '--enable': {
        'remote-sync': ['--debug', '--stacktrace'],
        'smtp': ['--debug', '--stacktrace']
    },
    '--disable':  {
        'remote-sync': ['--debug', '--stacktrace'],
        'smtp': ['--debug', '--stacktrace']
    },
    '--version':[],
    '--help':[],
    '--start': [],
    '--reset': [],
});
completion.init();

require('../src/cli').cli(process.argv);